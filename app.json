{
  "expo": {
    "name": "Astropot",
    "slug": "astropot",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a0b2e"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.suphi.astropot"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1a0b2e"
      },
      "package": "com.suphi.astropot",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "874cfada-d31a-42cf-a89e-5330f7970298"
      }
    }
  }
}
